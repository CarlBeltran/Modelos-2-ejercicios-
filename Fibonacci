def fibonacci(n):
 if n==1 or n==2:
  return 1
 return fibonacci(n-1)+fibonacci(n-2)

try:
    num = int(input("Que numero de la serie de Fibonacci desea conocer ?"))

    calculo= fibonacci(num)
    print "El numero", num ,"de la serie de fibonacci es", calculo
except:
	print "Se requiere un valor numerico"

